{
  "expo": {
    "name": "TiltTouchMouse",
    "slug": "TiltTouchMouse",
    "version": "1.2.0",
    "orientation": "landscape",
    "icon": "./assets/misonja.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "tilt.touch.mouse",
      "adaptiveIcon": {
        "foregroundImage": "./assets/misonja.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
